{
  "name": "Zoom",
  "documents": {
    "Terms of Service": {
      "fetch": "https://zoom.us/terms",
      "select": [
        ".HeaderForWysiwyg__content",
        ".WysiwygSection"
      ],
      "executeClientScripts": true
    },
    "Privacy Policy": {
      "fetch": "https://zoom.us/privacy",
      "select": [
        ".main-DefaultTemplate"
      ],
      "remove": [
        ".category"
      ]
    }
  }
}
